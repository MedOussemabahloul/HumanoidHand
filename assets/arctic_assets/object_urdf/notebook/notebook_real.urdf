<robot name="notebook">



  <link name="bottom">
    <visual name="bottom_visual0">
      <origin xyz="0 0 0" />
      <geometry>
        <mesh filename="bottom_real.obj" scale="0.001 0.001 0.001" />
      </geometry>
    </visual>
    <collision name="bottom_collision0">
      <origin xyz="0 0 0" />
      <geometry>
        <mesh filename="bottom_real.obj" scale="0.001 0.001 0.001" />
      </geometry>
    </collision>
    
  
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
</link>

  <link name="top">
    <visual name="top_visual0">
      <origin xyz="0 0.01 0.0" />
      <geometry>
        <mesh filename="top.obj" scale="0.0011 0.001 0.001" />
      </geometry>
    </visual>
    <collision name="top_collision0">
      <origin xyz="0 0 0" />
      <geometry>
        <mesh filename="top.obj" scale="0.0011 0.001 0.001" />
      </geometry>
    </collision>
    
  
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001" />
    </inertial>
</link>


  <joint name="joint1" type="revolute">
    <parent link="bottom" />
    <child link="top" />
    <origin rpy="0 0.0 0.2" xyz="-0.0 0.00 0.0" />
    <axis xyz="0 0 -1" />
    <limit lower="0" upper="3.14" />
    
  </joint>

</robot>